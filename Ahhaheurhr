local Toggle: one = Section:NewToggle({
    Name = "X-Ray Vision", 
    Description = "See through walls and objects", 
    CurrentState = false, 
    Callback = function(value)
        local player = game.Players.LocalPlayer

        -- Tunggu karakter jika belum ada
        local character = player.Character or player.CharacterAdded:Wait()

        -- Fungsi untuk mengatur X-ray pada part tertentu
        local function setPartXRay(part, enabled)
            if part:IsA("BasePart") and part.CanCollide and part.Transparency < 1 then
                -- Pastikan part bukan bagian dari karakter pemain
                if not part:IsDescendantOf(character) then
                    part.LocalTransparencyModifier = enabled and 0.7 or 0
                end
            end
        end

        -- Fungsi utama untuk mengatur X-ray
        local function setXRay(enabled)
            for _, part in ipairs(workspace:GetDescendants()) do
                setPartXRay(part, enabled)
            end
        end

        -- Bersihkan koneksi sebelumnya jika ada
        if xRayConnection then
            xRayConnection:Disconnect()
            xRayConnection = nil
        end

        -- Terapkan X-ray sesuai toggle state
        setXRay(value)

        -- Jika diaktifkan, sambungkan event untuk objek baru
        if value then
            xRayConnection = workspace.DescendantAdded:Connect(function(part)
                setPartXRay(part, true)
            end)
        end
    end,
})

