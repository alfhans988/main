local Tab: two = Window:NewTab({
    Name = "ESP Player | CHAM", -- Name of the tab | string, required
    Icon = "rbxassetid://1234" -- Icon for the tab button | string, optional
})

local Section: two = Tab:NewSection({
    Name = "Reset / Default", -- Name of the section | string, required
    Description = "Highlight Players", -- Description of the section | string, optional 
})

local espSettings = {
    Enabled = false,
    FillColor = Color3.new(1, 0, 0),
    OutlineColor = Color3.new(1, 1, 1),
    FillTransparency = 0.5,
    OutlineTransparency = 0,
    UpdateDelay = 0.1 -- Delay to reduce load
}

-- Optimized ESP function
local function UpdateESP()
    if not espSettings.Enabled then return end
    
    for _, player in pairs(game.Players:GetPlayers()) do
        if player ~= game.Players.LocalPlayer and player.Character then
            local humanoid = player.Character:FindFirstChildOfClass("Humanoid")
            if humanoid and humanoid.Health > 0 then -- Cek karakter valid
                local highlight = player.Character:FindFirstChild("ESP_Highlight") or Instance.new("Highlight")
                highlight.Name = "ESP_Highlight"
                highlight.FillColor = espSettings.FillColor
                highlight.OutlineColor = espSettings.OutlineColor
                highlight.FillTransparency = espSettings.FillTransparency
                highlight.OutlineTransparency = espSettings.OutlineTransparency
                
                -- Critical fix: Turn off unnecessary effects
                highlight.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
                highlight.Adornee = player.Character
                highlight.Parent = player.Character
            elseif player.Character:FindFirstChild("ESP_Highlight") then
                player.Character.ESP_Highlight:Destroy()
            end
        end
    end
end

-- Use debounce for updates
local espDebounce = false
local function SafeUpdateESP()
    if not espDebounce then
        espDebounce = true
        pcall(UpdateESP) -- Use pcall for error handling
        task.wait(espSettings.UpdateDelay)
        espDebounce = false
    end
end

local Button: two = Section:NewButton({
    Name = "Reset To Default Settings", -- Name of the button | string, required
    Description = "Reset Settings To Default Settings",  -- Description of the button | string, optional 
    Callback = function() -- code executed on interaction with the element | function,  optional
        espSettings = {
            Enabled = espSettings.Enabled, -- Maintain current toggle state
            FillColor = Color3.new(1,0,0),
            OutlineColor = Color3.new(1,1,1),
            FillTransparency = 0.5,
            OutlineTransparency = 0
        }
        UpdateAllESP()
    end,
})

local Button: two = Section:NewButton({
    Name = "Refresh ESP [For New Player]", -- Name of the button | string, required
    Description = "Example Description",  -- Description of the button | string, optional 
    Callback = function() -- code executed on interaction with the element | function,  optional
        UpdateAllESP()
    end,
})

