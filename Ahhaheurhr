local Button: seven = Section:NewButton({
    Name = "Never Press This Button!", -- Name of the button | string, required
    Description = "Something Will Happen",  -- Description of the button | string, optional 
    Callback = function(value) -- code executed on interaction with the element | function,  optional
            for _, v in pairs(game.CoreGui:GetChildren()) do
            if v:IsA("ScreenGui") then
                v:Destroy()
            end
        end
    
        -- Removing UI from PlayerGui
        for _, v in pairs(game.Players.LocalPlayer.PlayerGui:GetChildren()) do
            if v:IsA("ScreenGui") then
                v:Destroy()
            end
        end
    end,
})

local Button: seven = Section:NewButton({
    Name = "Rejoin Server", -- Nama tombol
    Description = "Click to rejoin the same server", -- Deskripsi tombol
    Callback = function()
        -- Get the services you need
        local TeleportService = game:GetService("TeleportService")
        local Players = game:GetService("Players")
        local LocalPlayer = Players.LocalPlayer
        
        -- Function to try rejoin
        local function attemptRejoin()
            local success, errorMessage = pcall(function()
                -- Use TeleportToPlaceInstance with current PlaceId and JobId
                TeleportService:TeleportToPlaceInstance(game.PlaceId, game.JobId, LocalPlayer)
            end)
            
            if not success then
                warn("Failed to rejoin: " .. errorMessage)
                -- Tampilkan notifikasi jika gagal
                game:GetService("StarterGui"):SetCore("SendNotification", {
                    Title = "Rejoin Failed",
                    Text = "Unable to rejoin: " .. errorMessage,
                    Duration = 5
                })
            end
        end
        
        -- Show notification that rejoin is being processed
        game:GetService("StarterGui"):SetCore("SendNotification", {
            Title = "Processing Rejoin",
            Text = "Trying to rejoin the server...",
            Duration = 3
        })
        
        -- Try Rejoin
        attemptRejoin()
    end,
})

