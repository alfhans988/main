})

local Toggle: five = Section:NewToggle({
    Name = "Save Character Settings",
    Description = "Preserve settings after death",
    CurrentState = savedSettings.Enabled,
    Callback = function(value)
        savedSettings.Enabled = value
    end,
})

-- Function to apply saved settings
local function applySavedSettings(character)
    if not savedSettings.Enabled then return end
    
    local humanoid = character:WaitForChild("Humanoid")
    humanoid.WalkSpeed = savedSettings.WalkSpeed
    humanoid.JumpPower = savedSettings.JumpPower
    Workspace.CurrentCamera.FieldOfView = savedSettings.FieldOfView
end

-- Apply settings when character appears
LocalPlayer.CharacterAdded:Connect(applySavedSettings)
if LocalPlayer.Character then
    applySavedSettings(LocalPlayer.Character)
end

local Section: five = Tab:NewSection({
    Name = "WalkSpeed Control", -- Name of the section | string, required
    Description = "Adjust movement speed", -- Description of the section | string, optional 
})

local Button: five = Section:NewButton({
    Name = "Heal Character", 
    Description = "Restores 50 health points", 
    Callback = function()
        -- Inisialisasi variabel
        local healAmount = 50
        local cooldown = 5 -- dalam detik
        local player = game.Players.LocalPlayer
        
        -- Cek apakah karakter sudah dimuat
        if not player.Character then
            warn("Character not loaded yet!")
            return
        end
        
        local humanoid = player.Character:FindFirstChild("Humanoid")
        if not humanoid then
            warn("Humanoid not found!")
            return
        end
        
        -- Cek apakah karakter sudah penuh health
        if humanoid.Health >= humanoid.MaxHealth then
            game:GetService("StarterGui"):SetCore("SendNotification", {
                Title = "Heal Failed",
                Text = "Health is already full!",
                Duration = 3
            })
            return
        end
        
        -- Eksekusi penyembuhan
        humanoid.Health = math.min(humanoid.Health + healAmount, humanoid.MaxHealth)
        
        -- Notifikasi
        game:GetService("StarterGui"):SetCore("SendNotification", {
            Title = "HEALED!",
            Text = "+"..healAmount.." HP (Now: "..humanoid.Health.."/"..humanoid.MaxHealth..")",
            Duration = 3,
            Icon = "rbxassetid://9921392666" -- optional (ganti dengan asset ID Anda)
        })
        
        -- Cooldown system
        Button:SetAttribute("Disabled", true)
        Button:SetAttribute("Description", "Cooldown: "..cooldown.."s")
        
        for i = cooldown, 1, -1 do
            task.wait(1)
            Button:SetAttribute("Description", "Cooldown: "..i.."s")
        end
        
        Button:SetAttribute("Description", "Restores 50 health points")
        Button:SetAttribute("Disabled", false)
    end
})

local Section: five = Tab:NewSection({
    Name = "WalkSpeed Control", -- Name of the section | string, required
    Description = "Adjust movement speed", -- Description of the section | string, optional 
})

local Button: five = Section:NewButton({
    Name = "Set Walkspeed To 50",
    Description = "Boost Your Speed",
    Callback = function()
        if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
            LocalPlayer.Character.Humanoid.WalkSpeed = 50
            savedSettings.WalkSpeed = 50
        end
    end,
})


local TextBox: five = Section:NewTextBox({
    Name = "Costum Walkspeed",
    PlaceholderText = "Input here..",
    Text = "16 Is Default, Click To Set",
    Trigger = "FocusLost",
    Callback = function(value)
        local numValue = tonumber(value)
        if numValue and LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
            LocalPlayer.Character.Humanoid.WalkSpeed = numValue
            savedSettings.WalkSpeed = numValue
        end
    end,
})

