local function CharacterAdded(player, character)
    if espSettings.Enabled then
        character:WaitForChild("Humanoid")
        task.wait(0.5) -- Jeda untuk memastikan karakter fully loaded
        SafeUpdateESP()
    end
end

-- Use a more efficient connection
local connections = {}
table.insert(connections, game.Players.PlayerAdded:Connect(function(player)
    table.insert(connections, player.CharacterAdded:Connect(function(character)
        CharacterAdded(player, character)
    end))
end))

-- Inisialisasi untuk pemain yang sudah ada
for _, player in pairs(game.Players:GetPlayers()) do
    if player.Character then
        CharacterAdded(player, player.Character)
    end
    table.insert(connections, player.CharacterAdded:Connect(function(character)
        CharacterAdded(player, character)
    end))
end

-- Cleanup saat script dimatikan
local function CleanupESP()
    for _, conn in ipairs(connections) do
        conn:Disconnect()
    end
    for _, player in pairs(game.Players:GetPlayers()) do
        if player.Character and player.Character:FindFirstChild("ESP_Highlight") then
            player.Character.ESP_Highlight:Destroy()
        end
    end
end

