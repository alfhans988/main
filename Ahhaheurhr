local Dropdown: two = Section:NewDropdown({
    Name = "Set ESP Outline Color [Light]",
    Description = "Set Light Color",
    Choices = {"Red", "Orange", "Yellow", "Green", "Cyan", "Blue", "Pink", "Purple", "Black", "Gray", "White"}, -- Hanya warna yang tersedia
    CurrentState = "White",
    Callback = function(choice)
        espSettings.OutlineColor = colorMaps.OutlineLight[choice]
        UpdateAllESP()
    end,
})

local Dropdown: two = Section:NewDropdown({
    Name = "Set ESP Outline Color [Dark]",
    Description = "Dark outlines for bright environments",
    Choices = {
        "Dark Black", "Dark Red", "Dark Blue", "Dark Green", 
        "Dark Purple", "Dark Gray", "Dark White", "Dark Cyan",
        "Dark Pink", "Dark Orange", "Dark Yellow"
    },
    CurrentState = "Dark Black",
    Callback = function(choice)
        espSettings.OutlineColor = colorMaps.OutlineDark[choice]
        
        -- Auto-adjust settings based on color
        if choice == "Dark Black" then
            espSettings.OutlineTransparency = 0.1
            espSettings.OutlineThickness = 2  -- Lebih tebal untuk hitam
        elseif choice == "Dark White" then
            espSettings.OutlineTransparency = 0.3
        else
            espSettings.OutlineTransparency = 0
        end
        
        UpdateAllESP()
        
        -- Beri notifikasi warna kurang visible
        if choice == "Dark Green" or choice == "Dark Blue" then
            game:GetService("StarterGui"):SetCore("SendNotification", {
                Title = "Color Tip",
                Text = "Use thicker outline for "..choice,
                Duration = 3
            })
        end
    end
})

local Slider: two = Section:NewSlider({
